{
	"_version": "1.8.0",
	"sap.app": {
		"id": "tireSelector",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},

		"dataSources": {
			"tireSelector_SRV.xsodata": {
				"uri": "/TireSelector_Xsodata/tireSelector/xsodata/tireSelector_SRV.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/tireSelector_SRV.xsodata/metadata.xml"
				}
			},
			"Z_TIRESELECTOR_SRV": {
				"uri": "/tireSelector-dest/node/Z_TIRESELECTOR_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/Z_TIRESELECTOR_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "tireSelector.view.master",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.38.34",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tireSelector.i18n.i18n"
				}
			},

			"XsodataModel": {
				"dataSource": "tireSelector_SRV.xsodata",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"preload": true,
				"settings": {
					"useBatch": false,
					"json": true,
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PUT"
				}
			},

			"FitmentModel": {
				"dataSource": "Z_TIRESELECTOR_SRV",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"preload": true,
				"settings": {
					"json": true,
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PUT"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "tireSelector.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Routemaster",
				"pattern": "Routemaster",
				"target": "Routemaster"
			}, {
				"name": "searchResults",
				"pattern": "searchResults",
				"target": "searchResults"
			}, {
				"name": "searchResultsTire",
				"pattern": "searchResultsTire/{modelData}",
				"target": "searchResultsTire"
			}, {
				"name": "searchResultsTireNoData",
				"pattern": "searchResultsTireNoData/{tireData}",
				"target": "searchResultsTireNoData"
			}, {
				"name": "productMarkups",
				"pattern": "productMarkups",
				"target": "productMarkups"
			}, {
				"name": "reportError",
				"pattern": "reportError",
				"target": "reportError"
			}, {
				"name": "tireQuotation",
				"pattern": "tireQuotation/{rowData}",
				"target": "tireQuotation"
			}],
			"targets": {
				"Routemaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "master",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"searchResults": {
					"viewType": "XML",
					"viewName": "searchResults",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"searchResultsTire": {
					"viewType": "XML",
					"viewName": "searchResultsTire",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"searchResultsTireNoData": {
					"viewType": "XML",
					"viewName": "searchResultsTire",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"productMarkups": {
					"viewType": "XML",
					"viewName": "productMarkups",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"reportError": {
					"viewType": "XML",
					"viewName": "reportError",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "tireSelector.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"tireQuotation": {
					"viewType": "XML",
					"viewName": "tireQuotation"
				}
			}
		}
	}
}